#include <stdio.h>

int main() {
    int num1, num2;

    printf("Digite o primeiro número: ");
    scanf("%d", &num1);

    printf("Digite o segundo número: ");
    scanf("%d", &num2);

    if (num2 != 0 && num1 % num2 == 0) {
        printf("O primeiro número é múltiplo do segundo.\n");
    } else {
        printf("O primeiro número NÃO é múltiplo do segundo.\n");
    }

    return 0;
}
